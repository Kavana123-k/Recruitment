
@using Kendo.Mvc.UI;


@(Html.Kendo().Grid((IEnumerable<Location>)TempData["Location"])
    .Name("grid")
    .Columns(columns =>
    {
        columns.Bound(p => p.id).Title("ID");
        columns.Bound(p => p.city).Title("City");




    })

 .Pageable()
    .Scrollable()
    .HtmlAttributes(new { style = "height:430px;" })
    .DataSource(dataSource => dataSource
        .Ajax()
        .PageSize(20)

))