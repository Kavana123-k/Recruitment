@model List<Vacancy>

@{
    ViewBag.Title = "Vacancies";
}

@*@{
    var data = ViewBag.Message;
 }*@


<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="stylesheet" href="~/css/layoutstyles.css">
    <link rel="stylesheet" href="~/css/tablestyles.css">
</head>
<body>
    <h2>Vacancies</h2>
    <div class="table-wrapper">
        <table class="fl-table">
            <thead>
                <tr>
                       <th>Id</th>
                    <th>Code</th>
                    <th>Position</th>
                    <th>Vacancies</th>

                </tr>
            </thead>
            <tbody>



                @foreach (var data in Model)
                {
                    <tr>
                        <td><a href="~/Home/InsertVacancies/@data.id" class="btn btn-default" onclick="Update">@data.id</a></td>
                        <td>@data.code</td>
                        <td>@data.name</td>
                        <td>@data.vacancy</td>
                    </tr>
                }
                </tbody>

        </table>
    
    </div>
        <div>
            <a asp-action="Index">Back to Dashboard</a>
        </div>


        @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
</body>

</html>